VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "SecondInventry"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Function getQuantity(jan As String)
'SecondInventry=ネット用在庫データシートの在庫数を取得
On Error Resume Next
    
    MatchRow = WorksheetFunction.Match(CStr(jan), SecondInventry.Range("SecondInventryCodeRange"), 0)
           
    If Err Then '1回目のMatch関数エラーは解除して、00を頭につけて検索し直す
        Err.Clear
        MatchRow = WorksheetFunction.Match("00" & CStr(jan), SecondInventry.Range("SecondInventryCodeRange"), 0)
    End If

    If Err Then '2回目のMatch関数エラーが出れば、Quantityは強制的に0
        getQuantity = 0
        Exit Function
    End If
    
    getQuantity = Cells(MatchRow, 3).Value

End Function
