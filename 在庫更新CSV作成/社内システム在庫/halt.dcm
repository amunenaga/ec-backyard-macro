VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "halt"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Sub setHalt()
Dim i As Long
i = 2

Do Until IsEmpty(halt.Cells(i, 3))
    
    Dim Code As Double
    Code = halt.Cells(i, 3).Value
    
    Dim Qty As Long
    Qty = halt.Cells(i, 7).Value
    
    Call yahoo6digit.overWriteQty(Code, Qty)
    
    i = i + 1
    
Loop

End Sub
