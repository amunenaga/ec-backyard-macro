VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "LastSecondInventry"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Function getQuantity(jan As String)
'SecondInventry=前回の棚無し在庫セット数を調べます

QtyCol = LastSecondInventry.Rows(1).Find("quantity").Column

On Error Resume Next
    
    MatchRow = WorksheetFunction.Match(CDbl(jan), LastSecondInventry.Range("LastQuantityCodeRange"), 0)
           
    If Err Then 'Match関数エラーは前回セット無しと見なす
        getQuantity = 0
        Exit Function
        
    End If
    
    getQuantity = Range(QtyCol & MatchRow).Value

End Function

