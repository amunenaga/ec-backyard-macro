VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "ThisWorkbook"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Option Explicit

Private Sub Workbook_AddinInstall()

Dim NewButton As CommandBarControl

Set NewButton = ThisWorkbook.Application.CommandBars("Worksheet Menu Bar").Controls.Add(Type:=msoControlButton)

With NewButton
    .Caption = "全てのファイルを印刷"
    .Style = msoButtonIconAndCaption
    .FaceId = 4
    .OnAction = "PrintAllPo"
    .Tag = "PrintAllBook"
End With

End Sub

Private Sub Workbook_AddinUninstall()
On Error Resume Next
CommandBars.FindControl(Type:=msoControlButton, Tag:="PrintAllBook").Delete

End Sub
